@keyframes spin
    0%
        transform: rotate(0deg);

    100%
        transform: rotate(1turn);

@keyframes bounce
    0%, 20%, 50%, 80%, 100%
        transform: translateY(0);

    40%
        transform: translateY(-15px);

    60%
        transform: translateY(-5px);

@keyframes pulse
    0%, 20%, 50%, 80%, 100%
        transform: scale(1);

    40%
        transform: scale(1.1);

    60%
        transform: scale(1.1);

@keyframes shake
    0%
        transform: translateY(0);

    20%, 40%, 60%, 80%, 100%
        transform: translateX(-3px);

    10%, 30%, 50%, 70%, 90%
        transform: translateX(3px);

@keyframes swing
    0%
        transform: rotate(20deg);

    50%
        transform: rotate(-10deg);

    100%
        transform: rotate(20deg);

@keyframes double-bounce
    0%
        transform: scale(0);

    50%
        transform: scale(1);

    100%
        transform: scale(0);

@keyframes spring
    0%
        transform: scale(1.1);

    40%
        transform: scale(.9);

    100%
        transform: scale(1);

@keyframes fadein
    0%
        opacity: 0;

    100%
        opacity: 1;

@keyframes ascend
    0%
        transform: translateY(30px);
        opacity: 0;

    60%
        transform: translateY(20px);
        opacity: 0;

    100%
        opacity: 1;
        transform: translateY(0);

@keyframes descend
    0%
        transform: translateY(-30px);
        opacity: 0;

    60%
        transform: translateY(-20px);
        opacity: 0;

    100%
        opacity: 1;
        transform: translateY(0);

@keyframes load2
    0%
        transform: rotate(0deg);

    100%
        transform: rotate(360deg);
