$boxShadow = {};
$boxShadow.distant = 0 4px 12px rgba(0, 0, 0, .1);
$boxShadow.close = 0 2px 3px 0 rgba(0, 0, 0, .1);
$boxShadow.subtle = 0 1px 2px 0 rgba(0, 0, 0, .2);
$boxShadow.form = 0 2px 2px 0 rgba(0, 0, 20, .015), 0 0 0 1px rgba(0, 0, 20, .0075);
$boxShadow.formFocus = 0 4px 4px 0 rgba(0, 0, 20, .025), 0 0 0 1px rgba(0, 0, 20, .01);
$boxShadow.emboss = inset 0 -2px 0 rgba(0, 0, 20, .15);
$boxShadow.deboss = inset 0 2px 0 rgba(0, 0, 20, .15);
$boxShadow.raise = 0 1px 1px 0 rgba(0, 0, 20, .05);
